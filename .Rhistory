rbind,
lapply(1:M, function(i) {
counts[i] <<- round(N * radii[i] / sum(radii[i:M]))
theta <- seq(0, pi, len = counts[i])
N <<- N - counts[i]
data.frame(
x = radii[i] * cos(theta), y = radii[i] * sin(theta), r = i,
theta = theta
)
})
)

library(ggparliament)
library(tidyverse)
library(patchwork)
ukresults <- read.csv("UK2017.csv")
ukresults <- ukresults %>%
filter(!is.na(win)) %>%
group_by(win) %>%
count(win)
ukresults$n <- as.numeric(ukresults$n)
left_parties <- c("lab","snp","lib","grn","plc", "ind","oth")
ukresults$location <-
ifelse(ukresults$win %in% left_parties, "left",
ifelse(ukresults$win==c("con", "dup"), "right",
NA  ))
ukresults_left <- ukresults %>%
filter(location=="left")
ukresults_right <- ukresults %>%
filter(location=="right")
leftside <- ggplot() + geom_parliament_dots(type="opposing_benches", seatspp = ukresults_left$n, party_name = ukresults_left$win, parlrows=10) + theme_parliament() + coord_flip() + scale_colour_party(palette="UK")
rightside <- ggplot() + geom_parliament_dots(type="opposing_benches", seatspp = ukresults_right$n, party_name = ukresults_right$win, parlrows=10)+ theme_parliament() + coord_flip() + scale_colour_party(palette="UK")
combine_opposingbenches(left = leftside, right = rightside)
americandata <- data(US_congress)
americandata <- US_congress
US_congress1 <- americandata %>%
filter(start_year=="2017" & chamber=="lower")
US_senate <- americandata %>%
filter(start_year=="2017" & chamber=="upper")
congress <- ggplot() + geom_parliament_dots(type='semicircle', totalseats=sum(US_congress1$seats), parlrows=10, seatspp=US_congress1$seats) + theme_parliament() + labs(colour="Party", title="United States Congress") + scale_colour_manual(values=c("#1c396e", "#ed1b35"), labels=c("D", "R")) #When I write scale_colour_party, Republicans show up as grey. Trying to fix it but in the meantime, there is a manual scale.
#It works below, however. :\
senate <- ggplot() + geom_parliament_dots(type='semicircle', totalseats=sum(US_senate$seats), parlrows=3, seatspp=US_senate$seats) + theme_parliament() + labs(colour="Party", title="United States Senate") +  scale_colour_party(palette="USA", labels=c("D", "R", "I"))
senate
congress
